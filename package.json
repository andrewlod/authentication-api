{
  "name": "authentication-api",
  "version": "0.0.1",
  "description": "Authentication REST API created with TypeScript",
  "main": "src/server.ts",
  "scripts": {
    "install-db": "npx prisma generate --schema ./src/database/schemas/schema.prisma",
    "migrate": "npx prisma migrate dev --schema ./src/database/schemas/schema.prisma",
    "start:dev": "npx nodemon",
    "start:prod": "node build/server.js",
    "build": "npm run install-db && tsc",
    "postbuild": "cd src && copyfiles database/schemas/**/* ../build",
    "swagger-autogen": "ts-node src/docs/swagger.ts",
    "generate-docs": "npx typedoc src/**/*.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewlod/authentication-api.git"
  },
  "keywords": [
    "REST",
    "API",
    "TypeScript"
  ],
  "author": "Andre Wlodkovski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewlod/authentication-api/issues"
  },
  "homepage": "https://github.com/andrewlod/authentication-api#readme",
  "devDependencies": {
    "@babel/core": "^7.24.1",
    "@babel/eslint-parser": "^7.24.1",
    "@babel/preset-env": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@jest/globals": "^29.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prisma": "^5.10.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.12",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.523.0",
    "@jest/types": "^29.6.3",
    "@prisma/client": "^5.10.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "supertest": "^6.3.4",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0"
  }
}
